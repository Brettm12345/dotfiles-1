# reload sxhkd
super + Escape
  pkill -USR1 -x sxhkd

super + Return
  urxvtc

super + space
  dmenu_run

super + shift + f
  firefox

super + shift + x
  slock

XF86AudioRaiseVolume
  mixer vol +5

XF86AudioLowerVolume
  mixer vol -5

XF86AudioMute
  mute

XF86MonBrightnessUp
  xbacklight -inc 10%

XF86MonBrightnessDown
  xbacklight -dec 10%

XF86AudioPlay
  mpc toggle

XF86AudioStop
  mpc stop

XF86AudioNext
  mpc next

XF86AudioPrev
  mpc prev

Print
  screenie
