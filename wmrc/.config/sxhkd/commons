# reload sxhkd
super + Escape
  pkill -USR1 -x sxhkd

super + Return
  urxvt

super + space
  dmenu_run

super + q
  showtodo

super + shift + f
  firefox

super + shift + x
  slock

XF86AudioRaiseVolume
  amixer -q sset Master 5%+ unmute

XF86AudioLowerVolume
  amixer -q sset Master 5%- unmute

XF86AudioMute
  amixer -q sset Master mute

XF86MonBrightnessUp
  xbacklight -inc 10%

XF86MonBrightnessDown
  xbacklight -dec 10%

XF86AudioPlay
  mpc toggle

XF86AudioStop
  mpc stop

XF86AudioNext
  mpc next

XF86AudioPrev
  mpc prev

Print
  screenie
