# reload sxhkd
super + Escape
  pkill -USR1 -x sxhkd

super + Return
  urxvt

super + space
  dmenu_wrapper run

super + q
  showtodo

super + shift + f
  $BROWSER

super + shift + x
  gotosleep

super + shift + p
  theme_menu

super + F1
  echo "over" > /tmp/minbar.cmd

super + F2
  echo "music" > /tmp/minbar.cmd

XF86AudioRaiseVolume
  echo `vol i` `vol +` > "$NOTIFY_FIFO_PATH"

XF86AudioLowerVolume
  echo `vol i` `vol -` > "$NOTIFY_FIFO_PATH"

XF86AudioMute
  echo `vol i` `vol m` > "$NOTIFY_FIFO_PATH"

XF86AudioMicMute
  echo `vol mic_icon` `vol u` > "$NOTIFY_FIFO_PATH"

XF86MonBrightnessUp
  xbacklight -inc 10%

XF86MonBrightnessDown
  xbacklight -dec 10%

XF86AudioPlay
  cmus-remote -u

XF86AudioStop
  cmus-remote -s

XF86AudioNext
  cmus-remote -n

XF86AudioPrev
  cmus-remote -r

Print
  screenie

Print + shift
  screenie -s

super + ctrl + k
  killw `pfw`

{_,shift + }button10
	screenie {_,-s}
